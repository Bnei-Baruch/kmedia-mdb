    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "express-watcher": "^1.0.3",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^8.0.1",
    "jest-axe": "^6.0.0",
