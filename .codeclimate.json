{
  "version":          "2",
  "checks":          {
		"method-complexity": {
			"config": {
				"threshold": 7
			}
		},
		"method-count": {
			"config": {
				"threshold": 30
			}
		},
		"method-lines": {
			"config": {
				"threshold": 50
			}
		},
		"similar-code": {
			"enabled": false
		}
  },
  "exclude_patterns": [
    "dist/",
    "**/node_modules/",
    "**/__tests__"
  ]
}
