{
  "version":          "2",
  "plugins":          {
  },
  "checks":          {
		"method-complxity": {
			"threshold": 7
		},
		"method-count": {
			"threshold": 30
		},
		"method-lines": {
			"threshold": 50
		},
		"similar-code": {
			"enabled": false
		}
  },
  "exclude_patterns": [
    "dist/",
    "**/node_modules/",
    "**/__tests__"
  ]
}
